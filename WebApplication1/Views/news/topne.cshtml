@model IEnumerable<DataLayer.Page>



    <section class="last-news-side border-radius">
        <header><i class="icons icon-star"></i> <h3> پربازدیدترین اخبار </h3></header>
       
        <ul>
            @foreach (var item in Model)
            {

                <li>
                    <img class="border-radius"    src="~/pageimge/@item.Imagename"  style="max-width:150px" />
                    <h2> <a href="">@item.Title</a></h2>
                    <span>انتشار : @item.CreatDate</span>
                </li>
            }
        </ul>
        </section>
