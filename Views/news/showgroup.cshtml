@model IEnumerable<DataLayer.show_group_vm>



<section class="category-section border-radius">
    <header><i class="fa fa-folder-open-o"></i> <h3> دسته بندی ها </h3></header>
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="/Group/@item.groupid/@item.title">@item.title <span>@item.pagecount</span></a></li>
        }

    </ul>
</section>
